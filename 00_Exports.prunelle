
if ( [ "`uname -s`" = "Darwin" ] )
then
	export DISPLAY=:0

	## Fink / DarwinPorts

	PATH=/bin:/usr/bin:/sbin:/usr/sbin
	MANPATH=/usr/share/man
	INFOPATH=/usr/share/info

	for i in usr/X11R6 dp sw usr/local ; do
		export PATH=$PATH:/$i/bin:/$i/sbin
		export MANPATH=$MANPATH:/$i/share/man
		export INFOPATH=$INFOPATH:/$i/share/info
	done

	alias aless='open -a /Applications/DarwinPorts/AquaLess.app'
fi

PS1_USER="1"
PS1_USER_SSH="$PS1_USER"
PS1_ROOT="31;1"
